    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "typestack": "./dist/cli.js"
